  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "prettier": "prettier -w .",
    "e2e": "ng e2e",
    "\nSTART --------": "",
    "start": "ng serve",
    "start:prod": "ng serve --configuration production",
    "start:lab": "ionic serve --lab --port 4200",
    "\nEMULATE --------": "",
    "emulate:android": "ionic capacitor run android --livereload --external",
    "emulate:ios": "ionic capacitor run ios --livereload --external",
    "\nPRE-BUILD --------": "",
    "pre-build:capacitor": "ionic capacitor sync",
    "\nBUILD --------": "",
    "build:android": "npm-run-all pre-build:* ; ionic capacitor build android",
    "build:ios": "npm-run-all pre-build:* ; ionic capacitor build ios",
    "\nBUILD PROD --------": "",
    "build-prod:android": "npm-run-all pre-build:* ; ionic capacitor build android --prod",
    "build-prod:ios": "npm-run-all pre-build:* ; ionic capacitor build ios --prod",
    "build-prod:pwa": "ionic build --prod -- --base-href https://timokluser-dev.github.io/UEK_M335_MyCrypto/",
    "\nDEPLOY --------": "",
    "deploy:pwa": "npm-run-all build-prod:pwa ; angular-cli-ghpages --dir=www"
  },
  "private": true,
